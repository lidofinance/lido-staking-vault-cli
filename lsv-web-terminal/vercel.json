{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/cli/route.ts": {
      "includeFiles": "configs/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/config",
      "destination": "/deployed-hoodi-vaults-testnet.json"
    },
    {
      "source": "/api/abi/:path*",
      "destination": "/abi/:path*"
    }
  ]
}
